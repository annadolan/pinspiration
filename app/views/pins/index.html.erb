
<%= javascript_include_tag 'pins.coffee' %>

<% if current_user %>
<%= link_to "Logout", logout_path %>
<% else %>
<%= link_to "Login", login_path %>
<%= link_to "Create Account", new_user_path %>
<% end %>


<div class="container">
  <div class="grid">
<% @pins.each do |pin| %>
  <div class="grid-item">
  <%#= pin.name %>
  <%= image_tag pin.image%>
  <p class="image-text"><%= "Source: #{pin.source}" %></p>

</div>

  <% end %>
</div>
</div>
