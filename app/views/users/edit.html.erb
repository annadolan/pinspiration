<%= form_for @user do |f| %>
  <%= f.label :avatar %>
  <%= f.text_field :avatar %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.submit "Update Profile" %>
<% end %>
